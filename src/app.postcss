html, body {
  margin: 0;
  padding: 0;
  height: 100%;
  color: #333;
  background-color: #eee;
  font-family: Helvetica, sans-serif;
}

// https://css-tricks.com/inheriting-box-sizing-probably-slightly-better-best-practice/
html {
  box-sizing: border-box;
}
*, *:before, *:after {
  box-sizing: inherit;
}

html, body, .App, .SlidesContainer {
  height: 100%;
}
